    "@nuxtjs/proxy": "^2.1.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@tinymce/tinymce-vue": "^3.2.8",
    "clipboard": "^2.0.8",
    "core-js": "^3.18.3",
    "cross-env": "^7.0.3",
    "date-fns": "^2.25.0",
    "js-cookie": "^3.0.1",
    "lodash-es": "^4.17.21",
    "nuxt": "^2.15.8",
    "perfect-scrollbar": "^1.5.2",
    "sweetalert2": "^11.1.9",
    "tinymce": "^5.10.0",
    "v-calendar": "^2.3.4",
    "v-video-embed": "^1.0.8",
    "vee-validate": "^3.4.13",
    "vue-nestable": "^2.6.0",

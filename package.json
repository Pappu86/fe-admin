    "lint-fix": "eslint --fix --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "prepare": "husky install",
    "lint": "yarn lint:js"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "dependencies": {
    "@mdi/js": "^6.3.95",
    "@nuxtjs/auth-next": "5.0.0-1624817847.21691f1",

{
  "name": "fe-admin",
  "version": "1.0.0",
  "description": "Admin panel for fe website.",
  "author": "Eliyas Hossain",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build --modern=client",
    "start": "nuxt start",
